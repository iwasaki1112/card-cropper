{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "out"
      }
    },
    {
      "src": "api/api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "dest": "frontend/out/_next/static/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "frontend/out/static/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/api.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/out/$1"
    }
  ]
}